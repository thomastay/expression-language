{
  "Expr": [
    {
      "name": "SingleString",
      "pattern": "'[^\\']*'"
    },
    {
      "name": "whitespace",
      "pattern": "\\s+"
    },
    {
      "name": "Bool",
      "pattern": "true|false"
    },
    {
      "name": "Op",
      "pattern": "and|not|or|\\+=|\\-=|\\*=|\\/=|\u003e=|\u003c=|==|!=|:|%|\u003e|\u003c|\\-|\\+|\\*|\\/|\\(|\\?|\\.|\\["
    },
    {
      "name": "EndExpr",
      "pattern": ",|\\)|\\]"
    },
    {
      "name": "Ident",
      "pattern": "[a-zA-Z]\\w*"
    },
    {
      "name": "Float",
      "pattern": "\\d*\\.\\d+(e\\d+)?"
    },
    {
      "name": "HexInt",
      "pattern": "0x[0-9a-fA-F_]+"
    },
    {
      "name": "OctInt",
      "pattern": "0o[0-7_]+"
    },
    {
      "name": "BinInt",
      "pattern": "0b[01_]+"
    },
    {
      "name": "Int",
      "pattern": "0|[1-9][\\d_]*"
    }
  ],
